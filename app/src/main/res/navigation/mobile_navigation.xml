<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">
    <action android:id="@+id/action_quizhome"
        app:destination="@id/nav_quizhome"/>
    <fragment
        android:id="@+id/nav_home"
        android:name="com.wazifate.wazifate.HomeScreen.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_quizhome"
        android:name="com.wazifate.wazifate.HomeScreen.ui.quiz_home.QuizHomeFragment"
        android:label="Quizzes"
        tools:layout="@layout/fragment_quiz_home" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.wazifate.wazifate.HomeScreen.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
